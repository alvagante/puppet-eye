  process '<%= @name %>' do
    pid_file '<%= @real_pidfile %>'
    start_command '<%= @real_startprogram %>'
    stop_command '<%= @real_stopprogram %>'
<% if @manage_cpu_check -%>
    check :cpu, :below => <%= @cpu_below %>, :times => <%= @cpu_times.inspect %>, :every => <%= @cpu_every %>.seconds
<% end -%>
<% if @manage_memory_check -%>
    check :memory, :below => <%= @memory_below %>.megabytes, :times => <%= @memory_times.inspect %>, :every => <%= @memory_every %>.seconds
<% end -%>
  end
